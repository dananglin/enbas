        "timeout": 30,
        "mediaTimeout": 60
    },
    "server": {
        "socketPath": "/var/run/user/1000/enbas/server.psqm2yeo.socket",
        "idleTimeout": 300
    },
    "integrations": {
        "browser": "firefox --new-window",
        "editor": "vim",
        "pager": "less -FIRX",
        "imageViewer": "feh --scale-down",
